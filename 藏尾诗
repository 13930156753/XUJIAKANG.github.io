#include<stdio.h>
#include<string.h>
int main()
{
    int i;
    char poem[4][20],p[20];
    for(i=0;i<4;i++)
    scanf("%s",poem[i]);
    for(i=0;i<4;i++)
    {   
        int k=strlen(poem[i]);
        p[2*i]=*(poem[i]+k-2);
        p[2*i+1]=*(poem[i]+k-1);
    }
    p[2*i]=0;
    printf("%s",p);
    return 0;
}
